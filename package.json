{
  "name": "react-webpack-babel-starter",
  "version": "0.1.0-alpha",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement) and Babel.",
  "keywords": [
    "react 16",
    "babel 6",
    "hot loader",
    "Webpack 3",
    "jsx",
    "jest 21",
    "express",
    "simple",
    "boilerplate",
    "node 8",
    "sass",
    "npm 5",
    "image loader",
    "HMR",
    "linter",
    "webpack-dev-server"
  ],
  "author": "starla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danwillscott/react16-boilerplate/"
  },
  "bugs": {
    "url": "https://github.com/danwillscott/react16-boilerplate/issues"
  },
  "homepage": "",
  "scripts": {
    "build": "webpack -p --config=configs/webpack/prod",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "jshint",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-env": "2.4.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.4",
    "express": "4.16.1",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "3.4.2",
    "jest": "21.2.1",
    "jshint": "2.9.5",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-webpack-plugin": "0.9.0",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "postcss-sass": "0.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/configs/jest/jest.preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "/__tests__/.*\\.(js|jsx)$",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/configs/jest/fileMock.js"
    },
    "globals": {
      "DEVELOPMENT": false,
      "FAKE_SERVER": false
    }
  }
}
